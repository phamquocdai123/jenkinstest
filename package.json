    "babel-preset-react": "^6.24.1",
    "hover.css": "^2.3.2",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "matchmedia-polyfill": "^0.3.2",
    "material-ui-flat-pagination": "^3.2.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.3",
    "react-countdown-now": "^2.1.0",
    "react-dom": "^16.8.3",
    "react-number-format": "^4.0.7",
    "react-player": "^1.9.3",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.1",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.2.0",
    "webrtc-adapter": "^7.2.1"
  },
  "scripts": {
    "start": "webpack",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
  
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "jest-junit": {
    "output": "output/coverage/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "create-react-component-folder": "^0.1.26",
    "jest-css-modules": "^2.1.0",
    "regenerator-runtime": "^0.13.2"
  }
}
